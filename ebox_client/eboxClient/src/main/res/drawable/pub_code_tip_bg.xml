<?xml version= "1.0" encoding= "utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <size
                android:width="788px"
                android:height="480px" />
            <stroke
                android:width="1px"
                android:color="@color/pub_black" />
            <corners android:radius="4px" />

            <solid android:color="@color/pub_grey" />

        </shape>
    </item>

    <item
        android:bottom="4px"
