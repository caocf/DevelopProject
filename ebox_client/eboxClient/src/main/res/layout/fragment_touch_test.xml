<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="20px"
    >

    <ImageView
        android:id="@+id/iv_touch_1"
        android:layout_width="60px"
        android:layout_height="60px"
        android:background="@color/pub_blue"
        />

    <ImageView
        android:id="@+id/iv_touch_2"
        android:layout_width="60px"
        android:layout_height="60px"
        android:layout_alignParentRight="true"
        />

    <ImageView
        android:id="@+id/iv_touch_3"
        android:layout_width="60px"
        android:layout_height="60px"
        android:layout_centerInParent="true"
        />

    <ImageView
        android:id="@+id/iv_touch_4"
        android:layout_width="60px"
        android:layout_height="60px"
        android:layout_alignParentBottom="true"
        />

    <ImageView
        android:id="@+id/iv_touch_5"
        android:layout_width="60px"
        android:layout_height="60px"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        />

    <com.ebox.pub.ui.customview.view.MaterialButton
        android:id="@+id/bt_ok"
        style="@style/bt_fragment_test"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="测试成功" />

    <com.ebox.pub.ui.customview.view.MaterialButton
        android:id="@+id/bt_error"
        style="@style/bt_fragment_test"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="测试失败"
        android:layout_toRightOf="@id/bt_ok"
        android:layout_marginLeft="10px"/>


</RelativeLayout>