<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="?attr/pub_top_nag_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.ebox.pub.ui.customview.Title
        android:id="@+id/title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />

    <com.handmark.pulltorefresh.library.PullToRefreshGridView
        android:id="@+id/tradingScrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="110px"
        android:layout_marginLeft="110px"
        android:layout_marginRight="110px"
        android:layout_marginTop="120px"
        android:cacheColorHint="#00000000"
        android:horizontalSpacing="30px"
        android:numColumns="2"
        android:scrollbars="none"
        android:verticalSpacing="46px" />

</LinearLayout>
