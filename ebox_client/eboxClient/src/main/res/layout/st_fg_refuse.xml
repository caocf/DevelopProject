<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    style="?attr/pub_top_nag_page"
    android:id="@+id/rootView">
    <com.ebox.pub.ui.customview.Title
        android:id="@+id/title" 
		android:layout_width="fill_parent" 
		android:layout_height="wrap_content" />
    
    
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title"
        android:gravity="center">
	    
	    <TextView 
	        android:id="@+id/tv_refuse_text" 
			android:layout_width="1240px"
	        android:layout_height="552px"
	        style="@style/pub_text_black_size_60_px"
			android:background="@color/pub_white"
			android:gravity="center"
			android:layout_marginTop="50px"/>

        <Button
            android:layout_width="1240px"
            android:layout_height="276px"
            android:background="@drawable/pub_code_btn_green"
            android:text="完善资料"
            style="@style/pub_text_white_size_60_px"
            android:id="@+id/st_btn_perfor"
            android:gravity="center"
            android:layout_marginTop="50px"
            android:layout_below="@+id/tv_refuse_text"/>

	        
	</RelativeLayout>

</RelativeLayout>